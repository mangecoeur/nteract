{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true,
    "mocha": true
  },
  "rules": {
      "camelcase": [0],
      "no-console": [2, { allow: ["warn", "error"] }],
      "no-underscore-dangle": [0]
  },
  "settings": {
      "import/core-modules": ["electron"]
  }
}
